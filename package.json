{
  "name": "mikechan-bot",
  "version": "1.0.0",
  "description": "ミケちゃんがslackの平和を守ります",
  "main": "./dist/index",
  "scripts": {
    "deploy": "node ./deploy.js",
    "build:debug": "gulp build",
    "build:release": "gulp release",
    "install": "npm run build:release",
    "start": "node ./dist/index",
    "update:win": "nodist use v6 && nodist global v6 && npm run update:global && npm run update:package",
    "update:global": "npm update -g npm",
    "update:package": "npm update -D && npm update -S && npm remove -S node-notifier && npm i -D node-notifier@4.5.0",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "engines": {
    "node": "6.x.x",
    "npm": "3.x.x"
  },
  "keywords": [],
  "author": "gao",
  "license": "MIT",
  "dependencies": {
    "@slack/client": "^3.5.4",
    "babel-polyfill": "^6.13.0",
    "camelize": "^1.0.0",
    "date-with-offset": "^1.0.1",
    "snakeize": "^0.1.0"
  },
  "devDependencies": {
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-preset-es2017": "^1.6.1",
    "babel-register": "^6.9.0",
    "del": "^2.2.1",
    "globby": "^6.0.0",
    "gulp": "git://github.com/gulpjs/gulp.git#4.0",
    "gulp-babel": "^6.1.2",
    "gulp-plumber": "^1.1.0",
    "gulp-sourcemaps": "^1.6.0",
    "node-notifier": "^4.5.0"
  }
}
